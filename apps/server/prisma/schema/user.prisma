model User {
    id            Int       @id @default(autoincrement())
    email         String    @unique
    name          String?
    phone         String?
    address       String?
    gender        String?
    dateOfBirth   DateTime?
    password      String
    createdAt     DateTime  @default(now())
    updatedAt     DateTime  @updatedAt
    deletedAt     DateTime?
    refreshTokens Token[]
}
